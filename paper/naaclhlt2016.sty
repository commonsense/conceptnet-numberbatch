    \put(\LenToUnit{\rightoffset},\LenToUnit{-\naaclruleroffset}){%right ruler
      \naaclruler{\naaclrulercount}}
  }
  \AtTextUpperLeft{%confidential
    \put(0,\LenToUnit{1cm}){\parbox{\textwidth}{\centering\naaclhv\confidential}}
  }
\fi
}

